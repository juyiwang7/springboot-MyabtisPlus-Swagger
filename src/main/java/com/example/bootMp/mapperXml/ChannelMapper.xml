<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bootMp.mapper.ChannelMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.bootMp.entity.Channel">
        <id column="id" property="id" />
        <result column="chain_id" property="chain_id" />
        <result column="orgs" property="orgs" />
        <result column="channel_name" property="channel_name" />
        <result column="user_account" property="user_account" />
        <result column="created" property="created" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, chain_id, orgs, channel_name, user_account, created
    </sql>

</mapper>
